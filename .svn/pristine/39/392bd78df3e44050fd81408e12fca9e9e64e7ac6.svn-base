class PerformanceLot {
  String cusip;
  String description;
  String sector;
  float ret;
  float weight;
  String country;
  
  public PerformanceLot(String[] data) {
    cusip = data[0];
    description = data[1];
    sector = data[2];
    ret = float(data[3]);
    weight = float(data[4]);
    country = data[5];
  }
}
